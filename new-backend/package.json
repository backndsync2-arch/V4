    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "serverless-http": "^3.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.0.0"
  },

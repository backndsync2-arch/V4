{
  "name": "sync2gear-backend-js",
  "version": "1.0.0",
  "description": "Sync2Gear Backend API - Node.js/Express with MongoDB",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "seed": "node seed-users.js",
